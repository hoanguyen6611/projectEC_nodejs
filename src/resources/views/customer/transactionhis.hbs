<div class="historybank">
  <div class="historybank-item inline-block bg-success">
    <h5 class="text-color border-tran">Giao dịch gần đây</h5>
    {{#if historyList}}
    <table class="table mt-4">
      <tbody>
        {{#each historyList}}
        <tr>
          <th scope="row">1</th>
          <td>{{this.thoiGian}}</td>
          <td>{{this.trangThai}}</td>
          <td>{{this.soTien}} VNĐ</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
    {{/if}}
  </div>
</div>